{
  "dockerComposeFile": "docker-compose.yml",
  "service": "shell",
  "workspaceFolder": "/app",
  "mounts": [
    "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind"
  ],
  "remoteEnv": {
    "AUTH0_CLIENT_ID": "${localEnv:CYPRESS_AUTH_URL}",
    "AUTH0_CLIENT_SECRET": "${localEnv:CYPRESS_AUTH_URL}",
    "CYPRESS_AUTH_URL": "${localEnv:CYPRESS_AUTH_URL}",
    "CYPRESS_AUDIENCE_URL": "${localEnv:CYPRESS_AUTH_URL}",
    "CYPRESS_AUTH_CLIENT_ID": "${localEnv:CYPRESS_AUTH_URL}",
    "CYPRESS_AUTH_CLIENT_SECRET": "${localEnv:CYPRESS_AUTH_URL}"
  },
  "shutdownAction": "stopCompose"
}
