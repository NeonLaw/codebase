        "clipboard-copy": "^4.0.1",
        "gatsby-plugin-react-axe": "^0.5.0",
        "jwt-decode": "^3.1.2",
        "react-player": "^2.7.2",
        "slate": "^0.59.0",
        "slate-history": "^0.59.0",
        "slate-react": "^0.59.0",
        "typescript": "^4.1.3",
        "voca": "^1.4.0"
    },
    "resolutions": {
        "graphql": "^14.0.1"
    }
}
